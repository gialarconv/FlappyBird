    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    scale: 1.1 1.1;
}

.pause__button-panel {
    top: 0;
    right: 0;
    width: 100px;
    height: 100px;
    position: absolute;
    margin-left: 12px;
    margin-right: 12px;
    margin-top: 12px;
    margin-bottom: 12px;
}

.pause__button-graphic {
    width: 100%;
